datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model GlAllocate {
  bcustomsource          BigInt @map("BCUSTOMSOURCE")
  haccttree              Int    @map("HACCTTREE")
  haccttreedetail        Int    @map("HACCTTREEDETAIL")
  hchart                 Int    @map("HCHART")
  hoffsetacct            Int    @map("HOFFSETACCT")
  ibalancebydetails      Int    @map("IBALANCEBYDETAILS")
  ibalancingsource       BigInt @map("IBALANCINGSOURCE")
  iconsolidatebooks      Int    @map("ICONSOLIDATEBOOKS")
  id                     Int    @id @default(autoincrement()) @map("HMY")
  idelta                 Int    @map("IDELTA")
  idetail                Int    @map("IDETAIL")
  iincludesourcesegments Int    @map("IINCLUDESOURCESEGMENTS")
  iincludeuserdeffields  Int    @map("IINCLUDEUSERDEFFIELDS")
  iintercompany          Int    @map("IINTERCOMPANY")
  ipropertysegment       Int    @map("IPROPERTYSEGMENT")
  ireverseje             Int    @map("IREVERSEJE")
  isqftcolumn            Int    @map("ISQFTCOLUMN")
  isuppresssegment       Int    @map("ISUPPRESSSEGMENT")
  iunpostbatch           Int    @map("IUNPOSTBATCH")
  scode                  String @map("SCODE")
  scolumnname            String @map("SCOLUMNNAME")
  sdatecolumn            String @map("SDATECOLUMN")
  sfrombook              String @map("SFROMBOOK")
  sname                  String @map("SNAME")
  snotes                 String @map("SNOTES")
  sreference             String @map("SREFERENCE")
  ssourcecolumnname      String @map("SSOURCECOLUMNNAME")
  ssourcedatecolumn      String @map("SSOURCEDATECOLUMN")
  ssourcetablename       String @map("SSOURCETABLENAME")
  ssourcetype            String @map("SSOURCETYPE")
  stablename             String @map("STABLENAME")
  stype                  String @map("STYPE")
  suserdef1              String @map("SUSERDEF1")
  suserdef2              String @map("SUSERDEF2")
  suserdef3              String @map("SUSERDEF3")
  suserdef4              String @map("SUSERDEF4")
  suserdef5              String @map("SUSERDEF5")
  suserdef6              String @map("SUSERDEF6")
  suserdef7              String @map("SUSERDEF7")
  suserdef8              String @map("SUSERDEF8")

  @@map("GLAllocate")
}

model GlAllocateDetail {
  dfactor          Float  @map("DFACTOR")
  hacct            Int    @map("HACCT")
  haccttreedetail  Int    @map("HACCTTREEDETAIL")
  hfromacct        BigInt @map("HFROMACCT")
  hglallocate      Int    @map("HGLALLOCATE")
  hoffsetacct      Int    @map("HOFFSETACCT")
  ibudget          BigInt @map("IBUDGET")
  id               Int    @id @default(autoincrement()) @map("HMY")
  iexclude         Int    @map("IEXCLUDE")
  igroup           BigInt @map("IGROUP")
  inegate          Int    @map("INEGATE")
  iorder           BigInt @map("IORDER")
  iptd             Int    @map("IPTD")
  sacctdescription String @map("SACCTDESCRIPTION")
  sacctnode        String @map("SACCTNODE")
  sbook            String @map("SBOOK")
  scalctype        String @map("SCALCTYPE")
  sformula         String @map("SFORMULA")
  snotes           String @map("SNOTES")
  spropertylist    String @map("SPROPERTYLIST")
  ssegment1        String @map("SSEGMENT1")
  ssegment10       String @map("SSEGMENT10")
  ssegment11       String @map("SSEGMENT11")
  ssegment12       String @map("SSEGMENT12")
  ssegment2        String @map("SSEGMENT2")
  ssegment3        String @map("SSEGMENT3")
  ssegment4        String @map("SSEGMENT4")
  ssegment5        String @map("SSEGMENT5")
  ssegment6        String @map("SSEGMENT6")
  ssegment7        String @map("SSEGMENT7")
  ssegment8        String @map("SSEGMENT8")
  ssegment9        String @map("SSEGMENT9")
  stocolumnname    String @map("STOCOLUMNNAME")
  stype            String @map("STYPE")

  @@map("GLAllocateDetail")
}

model GlAllocateHistory {
  hacct        Int      @map("HACCT")
  hbatch       BigInt   @map("HBATCH")
  hje          BigInt   @map("HJE")
  hppty        Int      @map("HPPTY")
  ibook        Int      @map("IBOOK")
  id           Int      @id @default(autoincrement()) @map("HTEMPLATE")
  sbegin       Float    @map("SBEGIN")
  sbeginbudget Float    @map("SBEGINBUDGET")
  sbudget      Float    @map("SBUDGET")
  smtd         Float    @map("SMTD")
  ssegment1    String   @map("SSEGMENT1")
  ssegment10   String   @map("SSEGMENT10")
  ssegment11   String   @map("SSEGMENT11")
  ssegment12   String   @map("SSEGMENT12")
  ssegment2    String   @map("SSEGMENT2")
  ssegment3    String   @map("SSEGMENT3")
  ssegment4    String   @map("SSEGMENT4")
  ssegment5    String   @map("SSEGMENT5")
  ssegment6    String   @map("SSEGMENT6")
  ssegment7    String   @map("SSEGMENT7")
  ssegment8    String   @map("SSEGMENT8")
  ssegment9    String   @map("SSEGMENT9")
  umonth       DateTime @map("UMONTH")

  @@map("GLAllocateHistory")
}

model GlapTemplate {
  binactive    Int      @map("BINACTIVE")
  bvalid       Int      @map("BVALID")
  dtlastposted DateTime @map("DTLASTPOSTED")
  dtrangefrom  DateTime @map("DTRANGEFROM")
  dtrangeto    DateTime @map("DTRANGETO")
  haccrualacct Int      @map("HACCRUALACCT")
  hcompany     Int      @map("HCOMPANY")
  hfunding     Int      @map("HFUNDING")
  hoffsetacct  Int      @map("HOFFSETACCT")
  hperson      Int      @map("HPERSON")
  iapr         Int      @map("IAPR")
  iaug         Int      @map("IAUG")
  id           Int      @id @default(autoincrement()) @map("HMY")
  idec         Int      @map("IDEC")
  idisplaytype Int      @map("IDISPLAYTYPE")
  idueday      Int      @map("IDUEDAY")
  ifeb         Int      @map("IFEB")
  ijan         Int      @map("IJAN")
  ijul         Int      @map("IJUL")
  ijun         Int      @map("IJUN")
  imar         Int      @map("IMAR")
  imay         Int      @map("IMAY")
  inov         Int      @map("INOV")
  ioct         Int      @map("IOCT")
  irecurday    Int      @map("IRECURDAY")
  isep         Int      @map("ISEP")
  istatus      Int      @map("ISTATUS")
  samount      Int      @map("SAMOUNT")
  scode        String   @map("SCODE")
  sdesc        String   @map("SDESC")
  sexpensetype String   @map("SEXPENSETYPE")
  snotes       String   @map("SNOTES")
  spostcode    String   @map("SPOSTCODE")

  @@map("GLAPTemplate")
}

model GlapTemplateDetail {
  camount       Float  @map("CAMOUNT")
  dpercent      Float  @map("DPERCENT")
  dretention    Float  @map("DRETENTION")
  hacct         Int    @map("HACCT")
  hcategory     Int    @map("HCATEGORY")
  hcontract     Int    @map("HCONTRACT")
  hcostcode     Int    @map("HCOSTCODE")
  hglaptemplate Int    @map("HGLAPTEMPLATE")
  hjob          Int    @map("HJOB")
  hproperty     Int    @map("HPROPERTY")
  hvattrantype  Int    @map("HVATTRANTYPE")
  id            Int    @id @default(autoincrement()) @map("HMY")
  snotes        String @map("SNOTES")
  ssegment1     String @map("SSEGMENT1")
  ssegment10    String @map("SSEGMENT10")
  ssegment11    String @map("SSEGMENT11")
  ssegment12    String @map("SSEGMENT12")
  ssegment2     String @map("SSEGMENT2")
  ssegment3     String @map("SSEGMENT3")
  ssegment4     String @map("SSEGMENT4")
  ssegment5     String @map("SSEGMENT5")
  ssegment6     String @map("SSEGMENT6")
  ssegment7     String @map("SSEGMENT7")
  ssegment8     String @map("SSEGMENT8")
  ssegment9     String @map("SSEGMENT9")

  @@map("GLAPTemplateDetail")
}
